Config { font = "-misc-fixed-*-*-*-*-10-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "grey"
       , position = Top
       , lowerOnStart = True
       , commands = [ 
         --Run Weather "EGPF" ["-t","<station>: <tempC>C","-L","18","-H","25","--normal","green","--high","red","--low","lightblue"] 36000
         Run Weather "KLGB" ["-t","<station>: <tempF>Â°F, rh: <rh>% (<hour>)","-L","18","-H","25","--normal","green","--high","red","--low","lightblue"] 36000
         -- , Run Network "eth0" ["-L","0","-H","32","--normal","green","--high","red"] 10
         , Run Network "eth1" ["-L","0","-H","32","--normal","green","--high","red"] 10
         , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
         , Run Memory ["-t","Mem: <usedratio>%"] 10
         , Run Battery ["-t","AC: <acstatus> - <left>% / <timeleft>",
                        "-L", "10", "-H", "80", "-p", "3",
                        "--", "-O", "<fc=green>On</fc>", "-o", "<fc=red>Off</fc>",
                        "-L", "-15", "-H", "-5",
                        "-l", "red", "-m", "blue", "-h", "green"
