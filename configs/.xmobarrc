         , Run Date "%r - %a %b %_d %Y" "date" 10
         ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%cpu% | %memory% * %swap% | %battery% | %eth1% }{ <fc=#ee9a00>%date%</fc>| %KLGB% | %uname%"
       }
